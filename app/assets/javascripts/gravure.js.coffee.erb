$(document).ready ->
    navigator.geolocation.getCurrentPosition (position) ->
        $.get <%= gravure_path %> + position.coords.latitude + "/" + position.coords.longitude + ".json", null, (ShopCastData) ->
                alert "test"
                for obj in ShopCastData
                    alert ("#{obj["name"]} #{obj["casts"][0]["name"]}")

